<table border="solid">
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Manufacturer</th>
			<th>Model</th>
		</tr>
	</thead>
	<tbody>
	    {{#each crew_member}}
		<tr>
			<td>{{fname}}</td>
			<td>{{lname}}</td>
			<td>{{manufacturer}}</td>
			<td>{{model}}</td>
		</tr>	
		{{/each}}
	</tbody>
</table>